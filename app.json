{
  "expo": {
    "name": "FizPay",
    "slug": "FizPay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/favicon-16x16.png",
    "scheme": "fizpay",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Precisamos da c√¢mera para ler QR Codes."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/favicon-16x16.png",
        "backgroundImage": "./assets/images/favicon-16x16.png",
        "monochromeImage": "./assets/images/favicon-16x16.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.pedrorusso.FizPay"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon-16x16.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/favicon-16x16.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "91c20379-ed90-4bb9-a3b5-0afc1568dd36"
      }
    }
  }
}
